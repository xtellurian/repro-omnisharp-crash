@page
@model repro_omnisharp_crash.DashboardPageModel

@{
    ViewData["Title"] = "Dashboard";
}

<div class="container">
    <div class=row>
        <div class="col m-3">
            <div class="alert alert-secondary" role="alert">
                Values are cached per hour.
            </div>
        </div>
    </div>
    <div class="row m-1">
        <div class="col-sm-3">
            <div class="card bg-light" style="width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">FirstProperty</h5>
                </div>
            </div>
        </div>
        <div class="col-3">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">Total</h5>
                    <h6 class="card-subtitle mb-2 text-muted">Count</h6>
                    <p class="card-text">@Model.Stats.FirstProperty</p>
                </div>
            </div>
        </div>
        <div class="col-3">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">Active</h5>
                    <h6 class="card-subtitle mb-2 text-muted">Count</h6>
                    <p class="card-text">@Model.Stats.FirstProperty</p>
                </div>
            </div>
        </div>
        <div class="col-3">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">Mean Price of Active</h5>
                    <h6 class="card-subtitle mb-2 text-muted">Dollars</h6>
                    <p class="card-text">@Model.Stats.FirstProperty</p>
                </div>
            </div>
        </div>
    </div>
    <div class="row m-1">
